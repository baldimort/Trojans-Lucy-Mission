  shorttitle = {On the Origin of the {{Trojan}} Asteroids},
  doi = {10.1006/icar.2000.6523},
  abstract = {We present analytic and numerical results which illustrate the effects of Jupiter's accretion of nebular gas and the planet's radial migration on its Trojan companions. Initially, we approximate the system by the planar circular restricted three-body problem and assume small Trojan libration amplitudes. Employing an adiabatic invariant calculation, we show that Jupiter's thirty-fold growth from a \$10 M\_$\backslash$oplus\$ core to its present mass causes the libration amplitudes of Trojan asteroids to shrink by a factor of about 2.5 to \$$\backslash$sim 40\%\$ of their original size. The calculation also shows that Jupiter's radial migration has comparatively little effect on the Trojans; inward migration from 6.2 to 5.2 AU causes an increase in Trojan libration amplitudes of \$$\backslash$sim4\%\$. In each case, the area enclosed by small tadpole orbits, if made dimensionless by using Jupiter's semimajor axis, is approximately conserved. Similar adiabatic invariant calculations for inclined and eccentric Trojans show that Jupiter's mass growth leaves the asteroid's eccentricities and inclinations essentially unchanged, while one AU of inward migration causes an increase in both of these quantities by \$$\backslash$sim 4\%\$. Numerical integrations confirm and extend these analytic results. We demonstrate that our predictions remain valid for Trojans with small libration amplitudes even when the asteroids have low, but},
  number = {2},
  journal = {Icarus},
  author = {Fleming, Heather J. and Hamilton, Douglas P.},
  month = dec,
  year = {2000},
  keywords = {Astrophysics},
  pages = {479--493},
  file = {/home/andrew/Zotero/storage/R4B8AWQ7/Fleming and Hamilton - 2000 - On the origin of the Trojan asteroids Effects of .pdf;/home/andrew/Zotero/storage/H6CMW7LD/0007296.html},
  annote = {Comment: Submitted to Icarus - 13 Figs}
}

@inproceedings{ShoemakerTrojanasteroidsPopulations1989,
  title = {Trojan Asteroids - {{Populations}}, Dynamical Structure and Origin of the {{L4}} and {{L5}} Swarms},
  abstract = {The origin of Trojan asteroids, their populations, and dynamical 
structures are examined. Data available of Trojan asteroids reveal that
the total population of Trojans of greater than 15-km diam is roughly
half that estimated for the main-belt asteroids. Two-thirds of the known
Trojans are in the L4 swarm. Bright Trojans are as numerous in the L5
swarm as in L4 swarm, but faint L5 Trojans are only half as numerous.
Similarities of characteristic orbital parameters among certain Trojans
indicate the presence of five and possibly as many as eight collisional
groups in the L4 swarm. It is suggested that the magnitude distribution
of L4 Trojans is probably a result of strong collisional evolution. It
is suggested that the present Trojans are chiefly fragments of Jupiter
planetesimals that were captured during an episode of heavy flux near
Jupiter during the dispersal of the planetesimal swarm.},
  author = {Shoemaker, Eugene M. and Shoemaker, Carolyn S. and Wolfe, Ruth F.},
  year = {1989},
  keywords = {Asteroid Belts,Dynamic Characteristics,Meteorite Collisions,Orbital Elements,Planetary Evolution,Planetary Orbits,Planetary Structure,Protoplanets,Trojan Orbits},
  pages = {487--523}
}

